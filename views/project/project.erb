<h1>Project: <%= project.name%></h1>
<div class="output-field">
  <label>Project Name: </label>
  <%= project.name %>
</div>    
<div class="output-field">
  <label>Start Date: </label>
  <%= project.start_date %>
</div>    
<div class="output-field">
  <label>End date: </label>
  <%= project.end_date %>
</div>    
<div class="output-field">
  <label>Description: </label>
  <%= project.description %>
</div>    
<div class="output-field">
  <label>Creator ID: </label>
  <%= creator.first_name %>
</div>    
<div class="output-field">
  <label>Project ID: </label>
  <%= project.id %>
</div> 
<div id="buttons">
<% if creator.id == current_user.id %>
  <a href="/project/<%= project.id %>/edit"><button>Edit Project</button></a>
  <form method="post" action="/project/<%= project.id %>/delete">
  <input type="hidden" name="_method" value="delete">
    <button>Delete Project</button>
  </form>
<% end %>
  <a href="/home"><button>Back</button></a>
  <a href="/project/<%= project.id %>/work_session"><button>Work Sessions</button></a>
  

</div>   
