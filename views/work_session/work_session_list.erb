<h1>Work Sessions For Project: <%= project_name %></h1>
<a href="/project/<%= project_id %>/work_session/new"><button>Add a session</button></a>
<br>
<% work_sessions.each_with_index do|work_session, index| %>
  <%=index + 1%>. <a href="/project/<%= project_id %>/work_session/<%= work_session.id %>"><%= work_session.description %>
  <%= work_session.date %> <%=((work_session.end_time - work_session.start_time)/3600).round(1) %> Hours  billable? <%=work_session.is_billable %></a>
  <br>
<% end %>
<a href="/home"><button>Back</button></a>
